{
  "defaultSeverity": "warning",
  "extends": [
    "tslint-microsoft-contrib/latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "prettier": true,
    /**
     * Controversial/Configurable rules.
     */
    "no-backbone-get-set-outside-model": false,
    "no-http-string": false,
    "import-name": false

  }
}
